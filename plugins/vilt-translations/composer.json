{
    "name": "3x1io/vilt-translations",
    "description": "Translations With DB & UI",
    "keywords": [
        "laravel",
        "translations",
        "ui",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\ViltTranslations\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "3x1io/vilt-admin": "^1.0",
        "spatie/laravel-translation-loader": "^2.7",
        "spatie/laravel-package-tools": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\ViltTranslations\\ViltTranslationsProvider"
            ]
        }
    },
    "prefer-stable": true
}
